{
    "app-id": "io.liri.Calculator",
    "branch": "@@APP_BRANCH@@",
    "runtime": "io.liri.Platform",
    "runtime-version": "@@LIRI_SDK_VERSION@@",
    "sdk": "io.liri.Sdk",
    "command": "liri-calculator",
    "tags": [ "nightly" ],
    "desktop-file-name-suffix": " (Nightly)",
    "finish-args": [
        "--share=ipc",
        "--socket=x11",
        "--socket=wayland",
        "--filesystem=home",
        "--device=dri",
        "--env=SESSION_MANAGER="
    ],
    "modules": [
        {
            "name": "calculator",
            "sources": [
                {
                    "type": "git",
                    "url": "git://github.com/lirios/calculator.git",
                    "branch": "@@GIT_TAG@@"
                },
                {
                    "dest-filename": "actual-autogen.sh",
                    "path": "qbs-autogen",
                    "type": "file"
                },
                {
                    "commands": [
                        "./actual-autogen.sh lirideployment.libDir:lib/qml"
                    ],
                    "dest-filename": "autogen.sh",
                    "type": "script"
                }
            ]
        }
    ]
}
